<!-- Contact Section -->
<section id="contact">
    <div class="container">
        <div class="row">
            <div class="col-lg-8 col-lg-offset-2">
                <h2 class="text-center">Contato</h2>
                <hr class="star-primary">
                <form method="post" novalidate>
                    <div class="row control-group">
                        <div class="form-group col-xs-12 controls">
                            <label>NOME</label>
                            <input type="text" class="form-control" placeholder="NOME" id="name" name="Nome" maxlength="55" required data-validation-required-message="Digite seu nome.">
                            <p class="help-block text-danger"></p>
                        </div>
                    </div>
                    <div class="row control-group">
                        <div class="form-group col-xs-12 controls">
                            <label>EMAIL</label>
                            <input type="email" class="form-control" placeholder="EMAIL" id="email" name="Email" maxlength="60" required data-validation-required-message="Digite seu email.">
                            <p class="help-block text-danger"></p>
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="select" class="control-label">ASSUNTO</label>
                        <select class="selectpicker form-control" name="Assunto" required>
                            <option value="ENTREVISTA">ENTREVISTA</option>
                            <option value="ELOGIO">ELOGIO</option>
                            <option value="OPORTUNIDADE">OPORTUNIDADE</option>
                            <option value="PROPAGANDA">PROPAGANDA</option>
                            <option value="SUGESTÃO">SUGESTÃO</option>
                            <option value="RECLAMAÇÃO">RECLAMAÇÃO</option>
                            <option value="OUTRO">OUTRO</option>
                        </select>
                    </div>

                    <div class="row control-group">
                        <div class="form-group col-xs-12 controls">
                            <label>MENSAGEM</label>
                            <textarea rows="5" class="form-control" placeholder="MENSAGEM" maxlength="200" id="message"  name="Mensagem" required data-validation-required-message="Digite a mensagem."></textarea>
                            <p class="help-block text-danger"></p>
                        </div>
                    </div>
                    <div id="success"></div>
                    <div class="row">
                        <div class="form-group col-xs-12">
                            <button type="submit"  class="btn btn-success btn-lg">Enviar</button>
                        </div>
                    </div>
                </form>
            </div>
        </div>
    </div>
</section>

